{
  "name": "template-kronos-component",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "{{main}}"
  },
  "description": "kronos module template",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "--overwrite-- c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "devDependencies": {
    "semantic-release": "^18.0.1",
    "c8": "^7.10.0",
    "ava": "^3.15.0",
    "documentation": "^13.2.5"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/template-kronos-component.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/template-kronos-component/issues"
  },
  "homepage": "https://github.com/arlac77/template-kronos-component#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-javascript",
      "arlac77/template-root",
      "arlac77/template-arlac77-github",
      "arlac77/template-ava-coverage",
      "arlac77/template-documentation"
    ],
    "usedBy": [
      "Kronos-Integration/endpoint",
      "Kronos-Integration/interceptor",
      "Kronos-Integration/interceptor-decode-json",
      "Kronos-Integration/interceptor-gzip",
      "Kronos-Integration/interceptor-line-header",
      "Kronos-Integration/interceptor-line-parser",
      "Kronos-Integration/interceptor-line-tokenizer-csv",
      "Kronos-Integration/interceptor-line-tokens2obj",
      "Kronos-Integration/interceptor-message-handler",
      "Kronos-Integration/interceptor-object-data-processor-row",
      "Kronos-Integration/interceptor-stream",
      "Kronos-Integration/interceptor-stream-obj2string",
      "Kronos-Integration/interceptor-webhook",
      "Kronos-Integration/message",
      "Kronos-Integration/service",
      "Kronos-Integration/service-admin",
      "Kronos-Integration/service-authenticator",
      "Kronos-Integration/service-bullmq",
      "Kronos-Integration/service-consul",
      "Kronos-Integration/service-health",
      "Kronos-Integration/service-http",
      "Kronos-Integration/service-ldap",
      "Kronos-Integration/service-logger-gelf",
      "Kronos-Integration/service-repositories",
      "Kronos-Integration/service-smtp",
      "Kronos-Integration/service-swarm",
      "Kronos-Integration/service-systemd",
      "Kronos-Integration/service-uti",
      "Kronos-Integration/test-interceptor"
    ],
    "properties": {
      "node_version": "16.13.0"
    }
  }
}